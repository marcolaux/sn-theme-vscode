{
  "name": "sn-vscode-theme",
  "description": "A theme for Standard Notes inspired by the VS Code Dark theme that is easy to the eyes.",
  "version": "1.1.25",
  "license": "GPL-3.0",
  "main": "dist/dist.css",
  "author": "Marco Laux",
  "sn": {
    "main": "dist/dist.css"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/hyphone/sn-theme-vscode.git"
  },
  "scripts": {
    "build": "node-sass src/main.scss dist/dist.css",
    "watch": "chokidar \"**/*.scss\" -c \"npm run build\" --initial && ./build debug"
  },
  "devDependencies": {
    "chokidar-cli": "^3.0.0",
    "node-sass": "^6.0.1"
  }
}
